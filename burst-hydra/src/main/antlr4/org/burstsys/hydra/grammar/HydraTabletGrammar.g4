//////////////////////////////////////////////////////////////////
// Antlr Grammar for Hydra Tablets
//////////////////////////////////////////////////////////////////

grammar HydraTabletGrammar;

import HydraLexerGrammar, HydraExpressionGrammar;

////////////////////////////////////////////////////////////////////////////////////////////////////////////
// declarations
// tablet <tablet_name> [ <primitive_value> ] : <join_path>
////////////////////////////////////////////////////////////////////////////////////////////////////////////

tabletDeclaration:
    TABLET LSB valuePrimitiveTypeDeclaration RSB ;
